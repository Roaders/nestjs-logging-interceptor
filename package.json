{
    "name": "nestjs-logging-interceptor",
    "version": "0.0.3",
    "description": "A simple NestJS interceptor to log input/output requests",
    "keywords": [
        "nestjs",
        "typescript",
        "nodejs",
        "logging",
        "interceptor"
    ],
    "author": "Giles Roadnight",
    "license": "ISC",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .ts,.d.ts,.js",
        "lint:fix": "eslint . --ext .ts,.d.ts,.js --fix",
        "prebuild-release": "npm run clean",
        "build-release": "concurrently --kill-others-on-fail npm:test npm:lint npm:build",
        "prepublishOnly": "npm run build-release",
        "prepack": "npm run build-release"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "@nestjs/common": ">=5",
        "express": ">=4",
        "rxjs": ">=6"
    },
    "devDependencies": {
        "@nestjs/platform-express": "^8.4.4",
        "@nestjs/testing": "^8.2.4",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "concurrently": "^7.1.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "jest": "^27.4.7",
        "prettier": "^2.6.1",
        "supertest": "^6.1.6",
        "ts-jest": "^27.1.2",
        "typescript": "^4.6.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Roaders/nestjs-logging-interceptor.git"
    },
    "bugs": {
        "url": "https://github.com/Roaders/nestjs-logging-interceptor/issues"
    },
    "homepage": "https://github.com/Roaders/nestjs-logging-interceptor#readme"
}
